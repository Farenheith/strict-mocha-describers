{
  "name": "strict-mocha-describers",
  "version": "1.0.0",
  "description": "The implementation of describers strongly oriented to the methods that will be tested",
  "main": "build/src/index.js",
  "directories": {
    "bin": "build",
    "test": "src",
    "lib": "src"
  },
  "files": [
    "build/**/*"
  ],
  "scripts": {
    "test": "mocha --require ts-node/register --recursive \"**/*.spec.ts\"",
    "test:coverage": "nyc npm test",
    "build": "tsc -p ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Farenheith/strict-mocha-describers.git"
  },
  "author": "@Farenheith",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Farenheith/strict-mocha-describers/issues"
  },
  "homepage": "https://github.com/Farenheith/strict-mocha-describers#readme",
  "devDependencies": {
    "@types/chai": "^4.2.4",
    "@types/sinon": "^7.5.0",
    "gts": "^1.1.0",
    "nyc": "^14.1.1",
    "ts-node": "^8.4.1",
    "tslint": "^5.20.0",
    "typescript": "^3.6.4"
  },
  "dependencies": {
    "chai": "^4.2.0",
    "mocha": "^6.2.2",
    "sinon": "^7.5.0",
    "sinon-chai": "^3.3.0"
  }
}
